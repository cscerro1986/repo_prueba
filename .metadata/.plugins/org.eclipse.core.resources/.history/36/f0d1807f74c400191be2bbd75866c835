#include <stdio.h>
#include <stdlib.h>
//pide dos numeros, los guarda en 2 variables dist, las suma y lo muestra
int main(void) {
	int numA,numB;
	printf("Ingrese el primer numero.\n");
	scanf("%d",&numA);
	printf("Ingrese el segundo numero.\n");
	scanf("%d",&numB);
	printf("\nLa suma de los numeros es : %d",numA+numB);
	return EXIT_SUCCESS;
}


