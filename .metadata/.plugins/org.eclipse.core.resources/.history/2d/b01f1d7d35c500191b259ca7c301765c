#include <iostream>
using namespace std;
int dimeNumero(int &resul);

int main() {
	int resultado;
	int numero1;
	int numero2;
	printf("Ingrese el numero1: ");
	scanf("%d",&numero1);
	printf("ingrese el numero2");
	scanf("%d",&numero2);
	resultado=numero1+numero2;
	printf("El resultado de la suma es %d",resultado);

	return 0;
}


int dimeNumero(int &resul){

	printf("dime un numero");
	scanf("%d",&resul);


}

