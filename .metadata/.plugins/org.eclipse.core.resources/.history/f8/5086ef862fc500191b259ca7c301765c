#include <stdio.h>
#include <stdlib.h>

int dimeNumero(int *num);

int main() {
	int numero;

	dimeNumero(&numero);
	printf("%d",numero);

	return 0;
}


int dimeNumero(int *num){
	printf("Ingrese el numero1: ");
	int numero1 =5;

	*num =numero1;

	return 0;
}
